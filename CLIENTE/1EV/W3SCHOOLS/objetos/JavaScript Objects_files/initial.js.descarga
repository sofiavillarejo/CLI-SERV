creative.showAd = function () {
  //cargo vista banner
    setTimeout(function(){
        document.getElementById('content').className = "show";
        document.getElementById('capaclick').className = "show";
        document.getElementById('loader').className = "hide";
        $('.show_video').delay(1000).fadeOut(200);
    },500);
    
    //click salida > mute al vídeo
    $('#capaclick').click(function(){
        player.mute();
        $('#sound img').attr('src','assets/images/mute.png');
        click = true;
    });
    
    //control de sonido
    var click = true;
    $('#sound').click(function(){
        if(click){
            $('#sound img').attr('src','assets/images/sound.png');
            click = false;
            player.unMute();
        }
        else{
            $('#sound img').attr('src','assets/images/mute.png');
            click = true;
            player.mute();
        }
    });
    
    console.log(navigator.userAgent);
    var nav = navigator.userAgent.toLowerCase();

    if( nav.indexOf('nt 6.3') != -1 || nav.indexOf('nt 6.1') != -1 ){
        $('#content #slider ul li.frame_t_3 p.title2 span').css({height:'30px'});
        $('#content #slider ul li.frame_t_3 p.title1 span').css({height:'32px'});
        $('#content #slider .oferta2 #descuento').css({height:'57px'});
        $('#content #slider .oferta2 #porcentaje').css({height:'53px'});
        $('#content #slider .oferta2 #porcentaje').css({height:'53px'});
        $('#content #slider .oferta2').css({height:'85px'});
    }
    
    if( nav.indexOf('nt 10.0') != -1 || nav.indexOf('nt 6.1') != -1 ){
        $('#content #slider .oferta2 #porcentaje').css({height:'53px'});
        $('#content #slider .oferta2').css({height:'85px'});
        $('#content #slider .oferta2 #texto2_2').css({marginTop:'0'});
        $('#content #slider ul li.frame_t_3 p.title2 span.title').css({fontSize:'13px'});
        
    }
    
    if(  nav.indexOf('nt 6.1') != -1 ){
        $('#content #slider .oferta2 #texto2_2').css({marginTop:'-5px'});
    }
};


